package Main;
import MemberManage.MemberManager;
public class MMP {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		MainMenu menu = new MainMenu();
		MemberManager MM = new MemberManager(50);
		while(true) {
			switch (menu.Issue()) {
			case 1:
				MM.AddMember();
			case 2:
				MM.ShowAllMembers();
			default:
				System.out.println("옳지 않은 값입니다. ");
			}
		}
	}

}
